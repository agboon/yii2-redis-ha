{
  "name" : "pyurin/yii2-redis-ha",
  "description" : "Basic redis-ha client",
  "version" : "1.0-dev",
  "require" : {
    "yiisoft/yii2-redis" : "*"
  },
  "authors" : [ {
    "name" : "Petr Yurin",
    "email" : "p.yurin@gmail.com"
  } ],
  "keywords" : [ "yii2", "redis", "sentinel" ],
  "support" : {
    "source" : "https://github.com/pyurin/yii2-redis-ha"
  },
  "autoload" : {
    "psr-4" : {
      "pyurin\\yii\\redisHa\\" : "src/"
    }
  },
  "scripts": {
    "post-update-cmd": ["echo [WARNING] Delete all .git dirs", "php -r \"rmdir('pyurin/yii2-redis-ha/.git');\""],
    "post-install-cmd": ["echo [WARNING] Delete all .git dirs", "php -r \"rmdir('pyurin/yii2-redis-ha/.git');\""]
  },
